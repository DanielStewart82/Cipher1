#include <stdio.h>

void myFunction(char *myString, int n);

int main()
{   int n;
    char myString[] = "thisisatest";
    
    n = (sizeof(myString))/(sizeof(myString[0]));
    
    myFunction(myString,n);
    
    
    return 0;
    
}
void myFunction(char *myString, int n){
    int i;
    for( i = 0 ; i <= n ; i++ ){
        
        printf("%c\n", myString[i]);
}
}
